<script>
  let texto;

  function fetchHTML() {
    fetch("/auth/google")
      .then(function(response) {
        return response.text();
      })
      .then(function(text) {
        console.log(text);
        document.getElementById('texto').innerHTML = text;
      });
  }

  const g = document.getElementById('g')
g.addEventListener('click', fetchHTML); 

function popup(proveedor) {
    window.open("/auth/" + proveedor, "Sign In", "width=985,height=735");
  }
</script>

